<krpano version="1.17" title="Kampus PENS" onstart="startup();">
<!--  -->
	<include url="../tour-viewer/xml/skin-style.xml" />
	<include url="../tour-viewer/xml/toolbar-right.xml" />
	<include url="../tour-viewer/xml/skin-layout.xml" />
	<include url="../tour-viewer/xml/layout-mobile.xml" />
	<include url="../tour-viewer/xml/layout-android.xml" />
	<include url="../tour-viewer/xml/skin-event.xml" />
	<include url="../tour-viewer/xml/skin-action.xml" />
	<include url="../tour-viewer/xml/context-menu.xml" />
	<include url="../tour-viewer/xml/plugin.xml" />

	<!-- set skin settings: bingmaps? gyro? thumbnail controlling? tooltips? -->
	<skin_settings bingmaps="false"
	               bingmaps_key=""
	               bingmaps_zoombuttons="false"
	               gyro="true"
	               thumbs_width="120" 
	               thumbs_height="80" 
	               thumbs_padding="10" 
	               thumbs_crop="0|40|240|160"
	               thumbs_opened="true"
	               thumbs_text="true"
	               thumbs_dragging="true"
	               thumbs_onhoverscrolling="false"
	               thumbs_scrollbuttons="false"
	               thumbs_scrollindicator="false"
	               thumbs_loop="false"
	               tooltips_thumbs="false"
	               tooltips_hotspots="true"
	               tooltips_mapspots="false"
	               controlbar_offset="20"
	               share_url="http://cdn2.eyrodock.com/panorama/Kampus-PENS/?html5=flash"
	               />

	<!-- set optional skin logo url -->
	<layer name="skin_logo" url="../tour-viewer/img/logo.png" scale="0.9" align="righttop" x="15" y="10" zorder="100" opened_onclick="openurl('http://eyropano.com',_blank);" />

	<autorotate enabled="true" waittime="3.0" speed="5" />
	<!--autorotate enabled="true" waittime="6.0" accel="6" speed="10" horizon="15" tofov="60" /-->

	<scene name="scene_Pos_Satpam_D4" title="Pos Satpam D4" onstart="" thumburl="pano-images/Pos_Satpam_D4.tiles/thumb.jpg" lat="" lng="" heading="">

		<view hlookat="100" vlookat="0" fovtype="MFOV" fov="120" maxpixelzoom="2.0" fovmin="70" fovmax="140" limitview="auto" />

		<preview url="pano-images/Pos_Satpam_D4.tiles/preview.jpg" />

		<image type="CUBE" multires="true" tilesize="512" progressive="false">
			<level tiledimagewidth="1304" tiledimageheight="1304">
				<cube url="pano-images/Pos_Satpam_D4.tiles/%s/l2/%v/l2_%s_%v_%h.jpg" />
			</level>
			<level tiledimagewidth="652" tiledimageheight="652">
				<cube url="pano-images/Pos_Satpam_D4.tiles/%s/l1/%v/l1_%s_%v_%h.jpg" />
			</level>
			<mobile>
				<cube url="pano-images/Pos_Satpam_D4.tiles/mobile_%s.jpg" />
			</mobile>
		</image>

		<!-- place your scene hotspots here -->
		<hotspot name="spot1" style="skin_hotspotstyle" ath="-164.125" atv="2.770" linkedscene="scene_Hall_Tengah" />
		<hotspot name="spot2" style="skin_hotspotstyle" ath="-61.739" atv="2.979" linkedscene="scene_training_center" />

	</scene>

	<data name="scene_Pos_Satpam_D4">
		Gedung Pos Satpam D4 terletak di sebelah pintu masuk bagian timur. Bersebelahan dengan ruangan Training Center dan tepat berada di depan gedung D4.[br/][br/]
		Klik tanda panah hitam untuk menuju navigasi selanjutnya -->
	</data>

	<scene name="scene_Hall_Tengah" title="Hall Tengah" onstart="" thumburl="pano-images/Hall_Tengah.tiles/thumb.jpg" lat="" lng="" heading="">

		<view hlookat="-50" vlookat="0" fovtype="MFOV" fov="120" maxpixelzoom="2.0" fovmin="70" fovmax="140" limitview="auto" />

		<preview url="pano-images/Hall_Tengah.tiles/preview.jpg" />

